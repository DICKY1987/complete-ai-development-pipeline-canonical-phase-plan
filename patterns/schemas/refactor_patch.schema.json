{
  "description": "Schema for refactor_patch pattern instances",
  "title": "refactor_patch Pattern Instance",
  "$id": "refactor_patch.schema.json",
  "properties": {
    "pattern_id": {
      "type": "string",
      "const": "PAT-REFACTOR-PATCH-001"
    },
    "doc_id": {
      "type": "string",
      "const": "DOC-PAT-REFACTOR-PATCH-SCHEMA-125",
      "pattern": "^DOC-[A-Z0-9-]+$"
    },
    "inputs": {
      "type": "object",
      "description": "Pattern-specific input parameters"
    },
    "outputs": {
      "type": "object",
      "description": "Pattern execution outputs"
    }
  },
  "required": [
    "doc_id",
    "pattern_id",
    "inputs"
  ],
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "doc_id": "DOC-PAT-REFACTOR-PATCH-SCHEMA-125"
}
