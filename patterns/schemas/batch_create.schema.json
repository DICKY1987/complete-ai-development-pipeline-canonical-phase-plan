{
  "description": "Schema for batch_create pattern instances",
  "title": "batch_create Pattern Instance",
  "$id": "batch_create.schema.json",
  "properties": {
    "pattern_id": {
      "type": "string",
      "const": "PAT-BATCH-CREATE-001"
    },
    "doc_id": {
      "type": "string",
      "const": "DOC-PAT-BATCH-CREATE-SCHEMA-111",
      "pattern": "^DOC-[A-Z0-9-]+$"
    },
    "inputs": {
      "type": "object",
      "description": "Pattern-specific input parameters"
    },
    "outputs": {
      "type": "object",
      "description": "Pattern execution outputs"
    }
  },
  "required": [
    "doc_id",
    "pattern_id",
    "inputs"
  ],
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "doc_id": "DOC-PAT-BATCH-CREATE-SCHEMA-111"
}
