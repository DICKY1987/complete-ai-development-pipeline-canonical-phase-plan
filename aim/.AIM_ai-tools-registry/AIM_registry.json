{
  "tools": {
    "jules": {
      "name": "Jules CLI",
      "detectCommands": [
        "jules",
        "%APPDATA%/npm/jules.cmd"
      ],
      "versionCommand": ["jules", "--version"],
      "configPaths": ["%USERPROFILE%/.jules"],
      "logPaths": ["%USERPROFILE%/.jules/cache/cli.log"],
      "capabilities": ["code_generation"],
      "adapterScript": "%AIM_REGISTRY_PATH%/AIM_adapters/AIM_jules.ps1"
    },
    "aider": {
      "name": "Aider CLI",
      "detectCommands": [
        "aider",
        "%USERPROFILE%/.local/bin/aider.exe"
      ],
      "versionCommand": ["aider", "--version"],
      "configPaths": ["%USERPROFILE%/.aider"],
      "logPaths": [],
      "capabilities": ["code_generation"],
      "adapterScript": "%AIM_REGISTRY_PATH%/AIM_adapters/AIM_aider.ps1"
    },
    "claude-cli": {
      "name": "Claude CLI",
      "detectCommands": [
        "claude",
        "%USERPROFILE%/.local/bin/claude.exe"
      ],
      "versionCommand": ["claude", "--version"],
      "configPaths": ["%USERPROFILE%/.claude"],
      "logPaths": [],
      "capabilities": ["code_generation"],
      "adapterScript": "%AIM_REGISTRY_PATH%/AIM_adapters/AIM_claude-cli.ps1"
    }
  },
  "crossToolRulesPath": "%AIM_REGISTRY_PATH%/AIM_cross-tool/AIM_coordination-rules.json"
}
